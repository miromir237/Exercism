def square_of_sum(number):
    # Function that returns the square of the sum of the first n natural numbers

    # Initialize the sum
    sum = 0

    # Loop through the first n natural numbers
    for i in range(1, number+1):
        # Add i to the sum
        sum += i
    
    return sum**2

def sum_of_squares(number):
    # Function that returns the sum of the squares of the first n natural numbers

    # Initialize the sum
    sum = 0

    # Loop through the first n natural numbers
    for i in range(1, number+1):
        # Add i^2 to the sum
        sum += i**2
    
    return sum

def difference_of_squares(number):
    # Function that returns the difference between the square of the sum and the sum of the squares of the first n natural numbers

    return square_of_sum(number)-sum_of_squares(number)